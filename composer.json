{
    "name": "indragunawan/packagist-mirror",
    "description": "Creates Packagist.org mirror site.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Indra Gunawan",
            "email": "hello@indra.my.id"
        }
    ],
    "autoload": {
        "psr-4": {
            "Indragunawan\\PackagistMirror\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "amphp/amp": "^2.0",
        "amphp/artax": "^3.0",
        "amphp/file": "^0.3.1",
        "symfony/config": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/lock": "^4.0",
        "tightenco/collect": "^5.6"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
